{
  "attr-bans": [
    "align",
    "background",
    "bgcolor",
    "border",
    "frameborder",
    "longdesc",
    "marginwidth",
    "marginheight",
    "scrolling",
    "style"
  ],
  "attr-name-ignore-regex": "^âš¡$",
  "attr-req-value": false,
  "attr-validate": true,
  "class-no-dup": true,
  "class-style": "dash",
  "href-style": "absolute",
  "id-class-style": "dash",
  "id-no-dup": true,
  "img-req-alt": true,
  "indent-style": "spaces",
  "indent-width": 2,
  "line-max-len": 100,
  "line-max-len-ignore-regex": "^\\s*(<style amp-boilerplate>.*?</style>|<script.*?></script>)",
  "line-no-trailing-whitespace": true,
  "spec-char-escape": false,
  "tag-bans": [],
  "tag-close": true,
  "tag-name-match": true,
  "tag-self-close": "never",
  "title-no-dup": true
}
